<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NutritionApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NutritionApp"
    xmlns:views="clr-namespace:NutritionApp.MVVM.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent Route="Login" Title="Login" ContentTemplate="{DataTemplate views:LoginPage}"/>
    <ShellContent Route="MainPage" Title="MainPage" ContentTemplate="{DataTemplate views:MainPage}"/>

</Shell>
